#############################################################
# CONEXUS LIRC Configuration                                #
# based on FBDVR401 remote with code adresses SAT1/SAT2/TV1 #
# based on FBTV401X remote with code adresses VCR1/VCR2/TV2 #
#############################################################

begin irexec

## ------------------------------------- ##
## --------------- CODES --------------- ##
## ------------------------------------- ##
begin
      prog   = irexec
      button = KEY_CODESAT1
      config = /boot/conexus.sh sat1 blink 
end

begin
      prog   = irexec
      button = KEY_CODESAT2
      config = /boot/conexus.sh sat2 blink 
end

begin
      prog   = irexec
      button = KEY_CODETV1
      config = /boot/conexus.sh tv1 blink 
end

begin
      prog   = irexec
      button = KEY_CODEVCR1
      config = /boot/conexus.sh vcr1 blink 
end

begin
      prog   = irexec
      button = KEY_CODEVCR2
      config = /boot/conexus.sh vcr2 blink 
end

begin
      prog   = irexec
      button = KEY_CODETV2
      config = /boot/conexus.sh tv2 blink 
end


## ------------------------------------- ##
## --------------- SAT 1 --------------- ##
## ------------------------------------- ##
begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_POWER
      config = /boot/conexus.sh sat1 standby 
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_MUTE
      config = /boot/conexus.sh sat1 mute 
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_SLEEP
      config = /boot/conexus.sh sat1 setsleeptimer:900
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_VOLUMEUP
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 vol+
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_VOLUMEDOWN
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 vol-
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_RIGHT
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 vol+
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_LEFT
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 vol-
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_UP
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 next
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_DOWN
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 previous
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_CHANNELUP
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 nextpreset
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_CHANNELDOWN
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 previouspreset
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_FORWARD
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 next
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_REWIND
      repeat = 2
      delay  = 4
      config = /boot/conexus.sh sat1 previous
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_PLAYPAUSE
      config = /boot/conexus.sh sat1 playpause
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_STOP
      config = /boot/conexus.sh sat1 pause
end

