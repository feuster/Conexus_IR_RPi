      button = KEY_HDMI
      config = /boot/conexus.sh sat1 mode:10
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_PIP
      config = /boot/conexus.sh sat1 mode:11
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_EPG
      config = /boot/conexus.sh sat1 mode:dab
end

