      button = KEY_TIMER
      config = /boot/conexus.sh sat1 mode:5
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_AUDIO
      config = /boot/conexus.sh sat1 mode:6
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_ZOOM
      config = /boot/conexus.sh sat1 mode:7
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_TEXT
      config = /boot/conexus.sh sat1 mode:8
end

begin
      prog   = irexec
      remote = FBDVR401_CODESAT1
      button = KEY_PAUSE
      config = /boot/conexus.sh sat1 mode:9
end

